{
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components",
    "./node_modules/prettier-stylelint/config.js"
  ],
  "plugins": [
  ],
  "processors": [
    "stylelint-processor-styled-components"
  ],
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": ["scrollbar-color", "scrollbar-width"]
      }
    ]
  }
}
